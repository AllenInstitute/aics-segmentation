    "        ax[channel].imshow(IMG[0,channel,MID_SLICE,:,:], cmap=plt.cm.gray)\n",
    "else:\n",
    "    ax.axis('off')\n",
    "    ax.imshow(IMG[0,0,MID_SLICE,:,:], cmap=plt.cm.gray)"
   ]
