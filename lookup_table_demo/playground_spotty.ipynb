   ],
   "source": [
    "FILE_NAME = '../../data/FBL/original/3500001486_100X_20171031_5-Scene-1-P18-F08.czi'\n",
    "\n",
    "reader = AICSImage(FILE_NAME) \n",
    "IMG = reader.data.astype(np.float32)\n",
    "\n",
    "print(IMG.shape)"
   ]
  },
  {
